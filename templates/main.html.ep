<!DOCTYPE html>
<html lang="en">
<head>
%= javascript '/jquery-2.0.3.js';
%= javascript '/underscore-min.js';
%= javascript '/jq.js';
%= stylesheet '//netdna.bootstrapcdn.com/bootstrap/3.0.3/css/bootstrap.min.css';
%= javascript '//netdna.bootstrapcdn.com/bootstrap/3.0.3/js/bootstrap.min.js';
</head>
<body>

<ol class='breadcrumb'>
    <li></li>
    <li>report</li>
    <li id='report'> </li>
    <li id='load_chapters'><a>chapter</a></li>
</ol>
<div id='main'></div>

<script id='chapter_row' type='text/template'>
<tr>
    <td><%%= number %></td>
    <td>
        <span class="btn btn-default chapter_link" chapter_identifier="<%%= identifier %>" href="<%%- uri %>"><%%= identifier %></a>
    </td>
    <td><%%= title %></td>
</tr>
</script>

<script id='finding_row' type='text/template'>
<tr>
    <td>
        <span class="btn btn-default finding_link" finding_identifier="<%%= identifier %>" href="<%%- uri %>"><%%= identifier %></a>
    </td>
    <td><%%= statement %></td>
</tr>
</script>

<script id='chapter_contents' type='text/template'>
<table>
    <tr><td> <span class="btn btn-default findings">findings</span> </td></tr>
    <tr><td> <span class="btn btn-default figures">figures</span> </td></tr>
    <tr><td> <span class="btn btn-default tables">tables</span> </td></tr>
    <tr><td> <span class="btn btn-default references">references</span> </td></tr>
</table>
</script>


</body>
</html>

